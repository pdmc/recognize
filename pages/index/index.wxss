/**index.wxss**/
.page{
    overflow: hidden;
    height: 100%;
    width: 100%;
}
.page .bg-box{
  position: absolute;
  z-index: 1;
  width: 100%;
  overflow: hidden;
}
.page .bg-box image{
  width: 100%;
  height: 100%;
}

.page .btn-box{
  text-align: center;
  position: absolute;
  z-index: 9999;
  width: 100%;
}
.page .btn-box image{
  width: 200rpx;
  height: 200rpx;
}
.page .btn-box button{
  width: 50%;
  margin-top:10px;
  color: #fff;
  background: #8c7464;
  /* background: none;
  color: #333; */
   border-radius: 18rpx; 
}
.page .change-box{
  position: absolute;
  top:0rpx;
  text-align: center;
  width: 100%;
  z-index: 9999;
}
 .page  .change-box .change-span{
  width: 35%;
  font-size: 30rpx;
}
.page  .change-box .change-span .weui-select{
  border: none;
} 